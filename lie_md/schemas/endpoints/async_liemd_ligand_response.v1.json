{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://mdstudio/schemas/gromacs_lie_response.json",
    "title": "GromacsLIE",
    "description": "Promised output of the LIE components using GROMACS MD software",
    "type": "object",
    "properties": {
	"status": {
	    "type": "string",
	    "description": "Current status of the future/promise",
	    "enum": [
		"completed",
		"failed",
		"running"
	    ]
	},
	"name": {
	    "type": "string",
	    "description": "docker name running Cerise cluster specialisation"
	},
	"port": {
	    "type": "integer",
	    "description": "Cerise webDAV connection port"
	},
	"job_name": {
	    "type": "string",
	    "description": "Cerise job name based on WAMP session"
	}, 
	"username": {
	    "type": "string",
	    "description": "Username for cluster account"
	},
	"workdir": {
	    "type": "string",
	    "description": "Path to the folder containing the results"
	},
	"task_id": {
	    "type": "string",
	    "description": "identifier of the whole job"
	},
	"job_type": {
	    "type": "string",
	    "description": "type of MD run",
	    "enum": [
		"solvent_ligand_md",
		"protein_ligand_md"
	    ]
	}
    },
    "required": [
	"status",
	"name",
	"job_name",
	"username",
	"job_type",
	"task_id"
    ]
}
