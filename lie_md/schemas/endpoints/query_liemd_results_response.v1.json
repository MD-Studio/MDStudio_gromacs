{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://mdstudio/schemas/gromacs_lie_response.json",
  "title": "GromacsLIE",
  "description": "Output of the LIE components using GROMACS MD software",
  "type": "object",
  "properties": {
	"status": {
	    "type": "string",
	    "description": "Current status of the future/promise",
	    "enum": [
		"completed",
		"failed",
		"running"
	    ]
	},
      "results": {
	  "type": "object",
	  "description": "Dictionary of MD result files"
      }
  },
    "required": [
	"status",
	"results"
    ]
}
